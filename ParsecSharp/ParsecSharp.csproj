<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.6;netstandard1.0</TargetFrameworks>
    <LangVersion>7.2</LangVersion>
    <RootNamespace>Parsec</RootNamespace>
    <DebugType Condition="'$(Configuration)'=='Release'">none</DebugType>
    <PackageId>ParsecSharp</PackageId>
    <Version>1.4.1</Version>
    <Authors>acple</Authors>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://github.com/acple/ParsecSharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/acple/ParsecSharp</PackageProjectUrl>
    <Description>Monadic parser combinator library for C#</Description>
    <Copyright>(c) 2015 acple</Copyright>
    <PackageTags>parser;combinator;linq;peg</PackageTags>
  </PropertyGroup>
</Project>
